		"@types/node": "^10.13.4",
		"@types/slug": "^0.9.1",
		"@types/sprintf-js": "^1.1.2",
		"chai": "^4.2.0",
		"mocha": "^6.2.2",
		"nyc": "^15.0.0",
		"prettier": "^1.19.1",
		"source-map-support": "^0.5.16",
		"ts-node": "^8.5.4",
		"tslint": "^5.20.1",
		"tslint-config-prettier": "^1.18.0",
		"typedoc": "^0.15.6",
		"typescript": "^3.7.2"
	},
