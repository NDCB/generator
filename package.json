		"immutable": "^4.0.0-rc.12",
		"iso-639-1": "^2.1.0",
		"js-yaml": "^3.13.1",
		"mime-types": "^2.1.25",
		"slug": "^2.0.0",
		"sprintf-js": "^1.1.2"
	}
}
